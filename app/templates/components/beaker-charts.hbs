<div class="flex-row">
  <h1>Time Series Charts</h1>
</div>
<div class="flex-row">
  {{#each metrics as |metric|}}
    {{beaker-chart data=metric.series title=metric.title}}
  {{/each}}
</div>
<div class="flex-row">
  <div>
    <label for="">Time Series Update Interval (seconds)</label>
    {{input type='number' min=30 value=updateIntervalDisplay}}
  </div>
</div>